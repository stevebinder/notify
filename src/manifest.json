{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "description": "Get GitHub notifications right in your browser bar.",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Notify for GitHub",
  "permissions": [
    "https://github.com/",
    "identity",
    "storage"
  ],
  "version": "0.0.2"
}